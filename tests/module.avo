# this file uses as module

const PI = 3.14159
const TAU = PI*2

func pow(a, b) {
    ## Returns a * a b times
    var result = 1;
    for var i = 0; i < b; ++i {
        result *= a;
    }
    return result;
}

func deg2rad(degrees) {
    return degrees * PI/180.0;
}
func rad2deg(radians) {
    return radians * 180.0/PI;
}
