func sort(arr) {
    var len = length(arr);
    var offset = 0;
    while offset < len {
        for var i = 0; i < len - offset; ++i {
            var j = i+1;
            if j < len and arr[j] < arr[i] {
                var tmp = arr[i];
                arr[i] = arr[j];
                arr[j] = tmp;
            }
        }
        ++offset;
    }
    return arr;
}

echo(sort([2, 5, 0, 1, 9]));
